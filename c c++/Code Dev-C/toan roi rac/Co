#include <iostream>
#include <cmath>
using namespace std;

void printChar(char b[], int k) {
    for (int i = 1; i <= k; i++)
        cout << b[i] << " ";
    cout << endl;
}

void hv_lap(char b[], int n, int k) {
    for (int i = 1; i <= k; i++)
        b[i] = 1;

    printChar(b, k);

    for (int c = 1; c < pow(n, k); c++) {
        int i = k;
        while (b[i] == n)
            i = i - 1;
        b[i] = b[i] + 1;
        for (int j = i + 1; j <= k; j = j + 1)
            b[j] = 1;
        printChar(b, k);
    }
}

int main() {
    char b[6] = {' ', 'xoai', 'cam', 'quit', 'oi', 'khe'};
    int n = 5;
    int k = 5;

    hv_lap(b, n, k);

    return 0;
}
