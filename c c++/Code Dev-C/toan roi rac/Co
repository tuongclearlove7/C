#include <iostream>
#include <algorithm>
using namespace std;

void printPermutations(string b[], int n) {
    do {
        for (int i = 0; i < n; i++) {
            cout << b[i] << " ";
        }
        cout << endl;
    } while (next_permutation(b, b + n));
}

int main() {
    string b[5] = {"xoai", "cam", "quit", "oi", "khe"};
    int n = 5;

    printPermutations(b, n);

    return 0;
}
